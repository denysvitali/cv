\newcommand{\eduentry}[7]{
  \begin{parcolumns}[colwidths={1=3cm, 2=10cm}]{2}
  \colchunk[1]{
    \begin{tabular}{@{}p{1.5cm}p{0.1cm}p{1.5cm}@{}}
    \color{date}\normalsize\raggedleft #1 & \color{date}- & \color{date}\normalsize\raggedright #2 \end{tabular}
  }
  \colchunk[2]{\textbf{#3} \\
    \emoji{1f393}{1em}
    \href{#5}{#4}\quad\emoji{1F4CD}{0.5em}
    #6 \\
    \\
    #7
  }
  \vspace{1em}
  \end{parcolumns}
}

