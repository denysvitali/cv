\newcommand{\cvheader}[2]{\begin{tabular}{@{}p{0.3\textwidth}p{0.65\textwidth}}
  #1 & #2 \\
\end{tabular}}
\newcommand{\profilepic}[1]{
  \raisebox{-\height}{\includegraphics[width=0.3\textwidth,height=0.3\textwidth]{#1}}
}
\newlength{\myhspace}

\newcommand{\flag}[1]{\makebox[1cm][l]{\raisebox{-0.2\height}{\includegraphics[height=1.2em]{pictures/emoji-flags/#1.png}}}}
\newcommand{\emoji}[2]{\raisebox{-0.2\height}{
    \def\svgwidth{#2}
    \setlength{\myhspace}{#2}

    \hspace{-\myhspace}
    \import{./pictures/emoji-tex/}{emoji_u#1.pdf_tex}
}}
\newcommand{\cvname}[1]{{\Huge\fontfamily{qag}#1}}
\newcommand{\cvsurname}[1]{{\Huge \fontfamily{qag}\textbf{#1}}}
\newcommand{\cvbirthday}[1]{\emoji{1f382}{1em} #1 \quad}
\newcommand{\cvflag}[2]{\flag{#2}\hspace{-5mm} #1}
\newcommand{\cvlocation}[1]{\emoji{1f3e1}{1em} #1 \quad}
\newcommand{\cvmobile}[1]{\emoji{1f4f1}{0.6em} #1 \quad}
